{
  "moduleFileExtensions": ["ts", "js", "json"],
  "rootDir": ".",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@modules/(.*)$": "<rootDir>/src/modules/$1",
    "^@user/(.*)$": "<rootDir>/src/modules/user/$1"
  },
  "testEnvironment": "node"
}
